<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.1.1//EN" "http://www.opensymphony.com/xwork/xwork-1.1.1.dtd">
<xwork>
	<package name="imanager-consume" extends="webwork-default" namespace="/consume">
		
		<interceptors>
			<interceptor-stack name="modelParamsStack">
				<interceptor-ref name="params" />
				<interceptor-ref name="model-driven" />
				<interceptor-ref name="checkLoginSessions" />
			</interceptor-stack>
		</interceptors>
		
		<action name="consumeAction" class="com.imanager.consume.action.ConsumeAction">
			<result name="error" type="velocity">/framework/error.vm</result>
			<result name="success" type="velocity">/framework/success.vm</result>
		
			<result name="doInitGetConsumeItemList" type="velocity">consume-consumeitem-list.vm</result>
			
			<result name="doGetConsumeItemList" type="velocity">consume-consumeitem-list.vm</result>
			
			<result name="doGetConsumItem" type="velocity">consume-consumeitem-update.vm</result>
			
			<result name="doUpdateConsumItem" type="redirect-action">
				<param name="namespace">/consume</param>
				<param name="actionName">consumeAction</param>
				<param name="method">doInitGetConsumeItemList</param>
			</result>
			
			<result name="doInitAddConsumItem" type="velocity">consume-add-consumeitem.vm</result>
			
			<result name="doAddConsumItem" type="redirect-action">
				<param name="namespace">/consume</param>
				<param name="actionName">consumeAction</param>
				<param name="method">doInitGetConsumeItemList</param>
			</result>
			
			<result name="doLogicDeleteConsumItem" type="redirect-action">
				<param name="namespace">/consume</param>
				<param name="actionName">consumeAction</param>
				<param name="method">doInitGetConsumeItemList</param>
			</result>
		
			<interceptor-ref name="modelParamsStack" />
		</action>
		
	</package>
</xwork>