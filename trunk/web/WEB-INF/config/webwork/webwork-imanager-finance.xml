<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.1.1//EN" "http://www.opensymphony.com/xwork/xwork-1.1.1.dtd">
<xwork>
	<package name="imanager-finance" extends="webwork-default" namespace="/finance">
		
		<interceptors>
			<interceptor-stack name="modelParamsStack">
				<interceptor-ref name="params" />
				<interceptor-ref name="model-driven" />
				<interceptor-ref name="checkLoginSessions" />
			</interceptor-stack>
		</interceptors>
		
		<action name="financeAction" class="com.imanager.finance.action.FinanceAction">
			<result name="error" type="velocity">/framework/error.vm</result>
			<result name="success" type="velocity">/framework/success.vm</result>
		
			<result name="doInitGetConsumeItemList" type="velocity">finance-consumeitem-list.vm</result>
			
			<result name="doGetConsumeItemList" type="velocity">finance-consumeitem-list.vm</result>
			
			<result name="doGetConsumItem" type="velocity">finance-consumeitem-update.vm</result>
			
			<result name="doUpdateConsumItem" type="redirect-action">
				<param name="namespace">/finance</param>
				<param name="actionName">financeAction</param>
				<param name="method">doInitGetConsumeItemList</param>
			</result>
			
			<result name="doInitAddConsumItem" type="velocity">finance-add-consumeitem.vm</result>
			
			<result name="doAddConsumItem" type="redirect-action">
				<param name="namespace">/finance</param>
				<param name="actionName">financeAction</param>
				<param name="method">doInitGetConsumeItemList</param>
			</result>
			
			<result name="doLogicDeleteConsumItem" type="redirect-action">
				<param name="namespace">/finance</param>
				<param name="actionName">financeAction</param>
				<param name="method">doInitGetConsumeItemList</param>
			</result>
		
			<interceptor-ref name="modelParamsStack" />
		</action>
		
	</package>
</xwork>