<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<title>iManager Contact</title>
		<script type="text/javascript" src="../resource/js/util.js"></script>
		<script type="text/javascript" src="../resource/js/contact/contact-type.js"></script>
	</head>
	
    <body  topmargin="10" bgcolor="#99CCFF">
    	<center>
			#parse("/framework/header.vm")
			<br/><br/><br/>
				
			<table border="0" width="80%" cellpadding="0" cellspacing="0">
        		<tr valign="top">
        			<td align="left" width="150">
        				#parse("/contact/menu.vm")
        			</td>
        			<td>&nbsp;&nbsp;</td>
        			<td align="center">
                    	<h2>添加联系类型</h2>
                    	<form id="contactTypeForm" action="contactTypeAction!addContactType.action" method="post">
                    		<table bgcolor="#E1E1E1" border="1" bordercolor="blue"width="100%" height="60"
                    			cellpadding="0" cellspacing="0" align="center" >
                    			<tr>
                    				<td align="right">联系类型:</td>
                    				<td align="left"><input type="text" id="contactType.contactType" name="contactType.contactType" onkeydown="KeyDown(event)" value="$esc.html($!contactType.contactType)" size="100" maxlength="100"></td>
                    			</tr>
                    			<tr>
									<td align="right">登录ID:</td>
                    				<td align="left">
											<input type="text" id="contactType.loginId" name="contactType.loginId" readonly="true"  value="$!contactType.loginId" size="20" maxlength="20">
									</td>
                    			</tr>
                    		</table>
                    		<input type="button" value="添加" onclick="checkContactTypeForm();" />&nbsp;&nbsp;
                    		<input type="reset" value="取消" />
                    	</form>
					</td>
				</tr>
			</table>
    	</center>
    </body>
</html>